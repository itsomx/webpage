{
  "name": "ITSO Website",
  "description": "ITSO Website main page",
  "license": "propietary",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org"
  }],
  "require": {
    "php": ">=5.4",
    "composer/installers": "^1.0",
    "wpackagist-plugin/akismet": "3.1.*",
    "wpackagist-plugin/jetpack": "^4.0",
    "wpackagist-plugin/redux-framework": "3.6.0.*",
    "wpackagist-theme/evolve": "3.5.*"
  },
  "require-dev": {
    "wpackagist/debug-bar": "0.8.*",
    "wpackagist/debug-bar-console": "~0.3"
  },
  "extra": {
    "installer-paths": {
      "htdocs/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "htdocs/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "htdocs/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "scripts": {
      "post-install-cmd": []
    }
}
