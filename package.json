    "eslint-config-standard-react": "2.3.*",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "5.0.*",
    "eslint-plugin-standard": "^1.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "1.0.*",
    "file-loader": "^0.8.4",
    "fs-extra": "0.30.*",
    "html-webpack-plugin": "2.16.*",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.8",
